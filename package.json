{
  "name": "gbl-web",
  "version": "0.0.12",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "export": "npm run build && next export",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run",
    "check": "npm-run-all --parallel check:ts format:ci lint",
    "check:ts": "tsc",
    "check:fix": "npm-run-all check:ts format lint:fix",
    "format": "prettier --write \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.(js|ts)\"",
    "format:ci": "prettier --list-different \"src/**/*.(js|ts|tsx|graphql|json)\" \"*.(js|ts)\"",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx src/"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.4.0",
    "autoprefixer": "10.2.5",
    "clsx": "1.1.1",
    "gray-matter": "4.0.3",
    "next": "10.1.3",
    "next-mdx-remote": "2.1.4",
    "postcss": "8.2.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "2.1.2"
  },
  "devDependencies": {
    "@types/node": "14.14.41",
    "@types/react": "17.0.3",
    "@uzh-bf/code-style-react-ts": "0.0.13",
    "npm-run-all": "4.1.5",
    "standard-version": "9.2.0",
    "typescript": "4.2.4"
  }
}
